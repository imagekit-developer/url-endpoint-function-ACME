{
  "name": "url-endpoint-function-template",
  "version": "1.0.0",
  "description": "Template for ImageKit URL Endpoint Functions with unit tests",
  "main": "handler.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "imagekit",
    "url-endpoint",
    "handler",
    "template"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "handler.js"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  }
}
